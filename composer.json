{
  "name": "roman/statistic-class",
  "description": "This is a project for statistic class.",
  "keywords": ["project", "template", "structure", "skeleton", "phpunit"],
  "type": "project",
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Roman Nagriy",
      "email": "karlson2006@ukr.net"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": ">=7.0",
    "symfony/console": "^4.1",
    "symfony/finder": "^4.1",
    "phpdocumentor/reflection-docblock": "^4.3"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.12"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Test\\": "tests/"
    }
  },
  "scripts": {
    "cs-fix": "php-cs-fixer fix --allow-risky=yes"
  }
}
